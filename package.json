{
    "name": "vue-min-picker",
    "version": "1.0.20",
    "description": "picker for mobile",
    "main": "lib/picker.common.js",
    "scripts": {
        "pub": "npm publish",
        "serve": "vue-cli-service serve",
        "start": "vue-cli-service serve",
        "dev": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint",
        "lib": "vue-cli-service build --target lib --name picker --dest lib packages/index.js"
    },
    "dependencies": {
        "core-js": "^3.6.5",
        "vue": "^2.6.11"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "~4.5.0",
        "@vue/cli-plugin-eslint": "~4.5.0",
        "@vue/cli-service": "~4.5.0",
        "babel-eslint": "^10.1.0",
        "eslint": "^6.7.2",
        "eslint-plugin-vue": "^6.2.2",
        "node-sass": "^5.0.0",
        "sass-loader": "^10.0.5",
        "style-loader": "^2.0.0",
        "vue-template-compiler": "^2.6.11"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "parserOptions": {
            "parser": "babel-eslint"
        },
        "rules": {
            "no-irregular-whitespace": "off"
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/leforyou/vue-min-picker.git"
    },
    "keywords": [
        "vue-min-picker picker pickers vue-picker vue-pickers vue-multi-picker mobile-picker mobile"
    ],
    "author": "MrLi",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/leforyou/vue-min-picker/issues"
    },
    "homepage": "https://github.com/leforyou/vue-min-picker#readme"
}